{
    "name": "TelegramTodoerBot",
    "description": "A telegram bot for saving your daily Tasks with an RPG-style twist.",
    "keywords": ["utility", "telegram"],
    "repository": "https://github.com/juvenileLad/tg-todoer-bot",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "Session string (pyrogram)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "MONGO_URL": {
            "description": "From https://cloud.mongodb.com/",
            "required": true
        }
    }
  }
